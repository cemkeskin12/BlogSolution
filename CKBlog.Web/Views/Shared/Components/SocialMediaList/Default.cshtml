@{
}
@model List<SocialMedia>


    <footer style="background: #1E2124">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <ul class="social-icons">
                        @foreach (var item in Model)
                        {
                        <li><a href="@item.Link">@item.Name</a></li>
                        }
                    </ul>
                </div>
            </div>
        </div>
    </footer>